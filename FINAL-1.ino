void setup() {
  Serial.begin(9600);
  display.begin(SSD1306_SWITCHCAPVCC, SCREEN_I2C_ADDR);
  servo1.attach(servoPin1);
  pinMode(fanPin, OUTPUT);
  pinMode(ePin, OUTPUT);
 // pinMode(BUZZER_PIN1, OUTPUT);
 pinMode(BUZZER_PIN2, OUTPUT);
  pinMode(ledPin, OUTPUT);
  Serial.begin(9600);
  Blynk.begin(auth,ssid,pass);
  dht.begin();
  lcd.init();
  lcd.backlight();
  lcd.setCursor(4, 1); 
  lcd.print("SMART CRADLE");
  delay(2000); 
}
void loop() {
  int micValue = analogRead(micPin);
  if(startTime==0){
   startTime=millis();
  }
  display.clearDisplay();
  display.drawBitmap(40, 8, frames[frame], FRAME_WIDTH, FRAME_HEIGHT, 1);
  display.display();
  frame = (frame + 20) % FRAME_COUNT;
  lcd.clear();  
  int temperature = dht.readTemperature();
  int humidity = dht.readHumidity();
  lcd.setCursor(2, 0); 
  lcd.print("TEM:"); 
  lcd.print(temperature);
  Blynk.virtualWrite(V0,temperature); 
  lcd.print("C"); 
  lcd.print(" HUM:"); 
  lcd.print(humidity);
  Blynk.virtualWrite(V1,humidity); 
  lcd.print("%"); 
  if (temperature >= tempThreshold) {
    digitalWrite(fanPin, HIGH);
   // delay(1000);
  } else {
    digitalWrite(fanPin, LOW);
  }
  if (micValue>=micThreshold){ 
    elapsedTime=millis()-startTime;
  } else{
   elapsedTime=0;
   startTime=millis();
  }
    if (elapsedTime >= 2000 && elapsedTime < 10000) {
     case1();
    } else if (elapsedTime >= 10000 && elapsedTime < 20000) {
     digitalWrite(ePin, HIGH);
     case2();
     digitalWrite(ePin, LOW);
    } else if (elapsedTime >= 20000) {
     case3();
    } else{
      case4();
    }
  Serial.print("temp:");
  Serial.print(temperature);
  Serial.println("C");
  Serial.print("hum:");
  Serial.print(humidity);
  Serial.println("%");
  Serial.println(micValue);
  timer.run();  // Initiates SimpleTimer
  Blynk.run();
}

void case1(void) {
  lcd.setCursor(2, 2);
  lcd.print("CRADLE IS MOVING");
  servo1.write(0);
  delay(1000);
  servo1.write(180);
  Serial.println("CRADLE IS MOVING");
  Blynk.virtualWrite(V3,"CRADLE IS MOVING");
}

void case2(void) {
  lcd.setCursor(3, 2); 
  lcd.print("PLAYING RHYMES");
  servo1.write(0);
  delay(1000);
  servo1.write(180);
  Serial.println("PLAYING RHYMES");
  Blynk.virtualWrite(V3,"PLAYING RHYMES");
}

void case3(void) {
  Blynk.logEvent("take_care");
  lcd.setCursor(1, 2); 
  lcd.print("TAKE CARE OF BABY");
  delay(1000);
  Serial.println("TAKE CARE OF BABY");
  Blynk.virtualWrite(V3,"TAKE CARE OF BABY");
  digitalWrite(ledPin, HIGH);
  digitalWrite(BUZZER_PIN2, HIGH);
  delay(1000);
  digitalWrite(ledPin, LOW);
  digitalWrite(BUZZER_PIN2, LOW);
}
void case4(void) {
  servo1.write(90);
  lcd.setCursor(2, 2);
  lcd.print("BABY IS SLEEPING");
  Serial.println("BABY IS SLEEPING");
  Blynk.virtualWrite(V2,"BABY IS SLEEPING");
  delay(1000);
}
